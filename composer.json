{
    "name": "binafy/laravel-stub",
    "description": "Generate stub files easy",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "2.x-dev",
        "orchestra/testbench": "8.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Binafy\\LaravelStub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "milwad-dev",
            "email": "milwad.dev@gmail.com",
            "role": "author",
            "homepage": "https://github.com/milwad-dev"
        },
        {
            "name": "binafy",
            "role": "owner",
            "homepage": "https://github.com/binafy"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
