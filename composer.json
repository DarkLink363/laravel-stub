{
    "name": "binafy/laravel-stub",
    "description": "Generate stub files easy",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "2.x-dev",
        "orchestra/testbench": "8.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Binafy\\LaravelStub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "miladev-ent",
            "email": "98118400+milwad-dev@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
